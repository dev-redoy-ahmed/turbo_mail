{
  "name": "turbo-mails",
  "version": "1.0.0",
  "description": "Temporary mail service with admin panel",
  "main": "server.js",
  "scripts": {
    "dev": "concurrently \"node server.js\" \"next dev\"",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "deploy": "node deploy.js",
    "start:api": "node server.js",
    "start:smtp": "node smtp-server.js",
    "start:redis": "redis-server redis.conf",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "test:smtp": "node -e \"console.log('Testing SMTP...'); require('./smtp-server.js');\"",
    "health": "curl -f http://localhost:5000/api/health || exit 1"
  },
  "dependencies": {
    "next": "^14.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "express": "^4.18.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "axios": "^1.6.0",
    "uuid": "^9.0.0",
    "moment": "^2.29.0",
    "nodemailer": "^6.9.0",
    "smtp-server": "^3.13.0",
    "ioredis": "^5.3.0",
    "mailparser": "^3.6.0",
    "socket.io": "^4.7.0",
    "socket.io-client": "^4.7.0",
    "node-cron": "^3.0.0",
    "dns": "^0.2.2",
    "nodemailer": "^6.9.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "autoprefixer": "^10.0.0",
    "concurrently": "^8.0.0",
    "eslint": "^8.0.0",
    "eslint-config-next": "^14.0.0",
    "nodemon": "^3.0.0",
    "postcss": "^8.0.0",
    "tailwindcss": "^3.0.0",
    "typescript": "^5.0.0"
  }
}